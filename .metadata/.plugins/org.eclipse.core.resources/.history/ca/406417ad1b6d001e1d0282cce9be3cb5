package proyecto;
import javax.swing.JTextField;
import javax.swing.JLabel;

public abstract class LoginView extends View {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private JTextField textField;
	private JTextField textField_1;

	public LoginView(App appController) {
		super(appController);
		setLayout(null);
		
		JLabel lblEmail = new JLabel("Email:");
		lblEmail.setBounds(118, 88, 85, 19);
		add(lblEmail);
		
		textField = new JTextField();
		textField.setBounds(118, 109, 174, 20);
		add(textField);
		textField.setColumns(10);
		
		JLabel lblPassword = new JLabel("Contrase√±a:");
		lblPassword.setBounds(118, 148, 70, 14);
		add(lblPassword);
		
		textField_1 = new JTextField();
		textField_1.setBounds(118, 173, 174, 20);
		add(textField_1);
		textField_1.setColumns(10);
		
	}
}
